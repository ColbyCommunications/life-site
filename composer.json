{
    "name": "colbycommunications/life-site",
    "description": "Configuration project for the life.colby.edu site",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "6.2.2",
        "wpackagist-plugin/classic-editor": "1.6.3",
        "wpackagist-plugin/elementor": "3.15.3",
        "wpackagist-plugin/capability-manager-enhanced": "2.8.1",
        "wpackagist-plugin/tablepress": "1.14",
        "wpackagist-plugin/header-footer": "3.2.5",
        "wpackagist-plugin/duplicate-post": "4.5",
        "wpackagist-plugin/wordpress-seo": "19.14",
        "wpackagist-plugin/cloudflare": "4.11.0",
        "wpackagist-plugin/public-post-preview": "2.10.0",
        "colbycommunications/colby-life-theme": "dev-master",
        "wpackagist-plugin/wp-saml-auth": "2.1.3",
        "wpackagist-plugin/siteimprove": "1.2.1",
        "wpackagist-plugin/query-monitor": "3.12.3",
        "wpackagist-plugin/custom-post-type-ui": "1.13.5",
        "wp-cli/wp-cli-bundle": "^2.4",
        "psy/psysh": "^0.10.4",
        "colbycommunications/advanced-custom-fields-pro": "5.12.3",
        "colbycommunications/elementor-pro": "3.15.1",
        "colbycommunications/jet-elements": "2.6.2",
        "colbycommunications/gravityforms": "2.6.9",
        "colbycommunications/colby-ticket": "1.0.1",
        "colbycommunications/colby-clubs": "1.0.0",
        "wpackagist-plugin/wp-search-with-algolia": "2.5.1",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-theme/hello-elementor": "2.7.1"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-life-theme"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.colby.edu"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wordpress-seo"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": true
    }
}
